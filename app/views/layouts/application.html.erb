<!DOCTYPE html>
<html>
  <head>
    <title>RescueMission</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if flash[:notice] %>
    <h1 style="color: red"><%= flash[:notice] %></h1>
    <% end %>
    <%= yield %>
  </body>
</html>
